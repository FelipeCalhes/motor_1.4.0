PROCEDURE "UPSERT_REGIOES"() 
  LANGUAGE SQLSCRIPT 
  SQL SECURITY INVOKER AS 
  --DEFAULT SCHEMA <default_schema_name>
BEGIN 
    UPSERT REGRASNAMESPACE_REGIOES
        SELECT 
            REGIAO,
            MUNICIPIO_MUNICIPIO
        FROM REGRASNAMESPACE_REGIOES_TRANSITORIA;
    DELETE FROM REGRASNAMESPACE_REGIOES_TRANSITORIA;
    COMMIT WORK;
END